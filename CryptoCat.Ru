import random
import math
import json
import os
from datetime import datetime
import hashlib

class CryptoCats:
    """
    –ö–æ—Ç-—à–∏—Ñ—Ä–æ–≤–∞–ª—å—â–∏–∫ 5.0: —Å —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ–º –æ—Ç–ø–µ—á–∞—Ç–∫–æ–≤ –ª–∞–ø!
    –ö–∞–∂–¥—ã–π —Å—Ç–∏–ª—å –æ—Å—Ç–∞–≤–ª—è–µ—Ç —É–Ω–∏–∫–∞–ª—å–Ω—ã–π '—Å–ª–µ–¥' –≤ —Ñ–∞–π–ª–µ.
    """
    
    def __init__(self, seed=None, name="–ö—Ä–∏–ø—Ç–æ–ö–æ—Ç", style="random", save_dir="paw_prints"):
        """–ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∫–æ—Ç–∞ —Å –ø–∞–ø–∫–æ–π –¥–ª—è —Å–ª–µ–¥–æ–≤."""
        if seed is None:
            seed = random.randint(0, 10000)
            print(f"üê± {name} –≤—ã–±—Ä–∞–ª seed: {seed} –∏ —Å—Ç–∏–ª—å '{style}'")
        
        random.seed(seed)
        self.seed = seed
        self.name = name
        self.style = style
        self.paw_signature = "=^..^= ~~"
        self.history = []
        self.save_dir = save_dir
        
        # –°–æ–∑–¥–∞—ë–º –ø–∞–ø–∫—É –¥–ª—è —Å–ª–µ–¥–æ–≤, –µ—Å–ª–∏ –µ—ë –Ω–µ—Ç
        os.makedirs(save_dir, exist_ok=True)
        
        # –°—Ç–∏–ª–∏
        self.style_descriptions = {
            'random': "–ö–ª–∞—Å—Å–∏—á–µ—Å–∫–∏–π —Å–ª—É—á–∞–π–Ω—ã–π —Ç–æ–ø–æ—Ç",
            'careful': "–û—Å—Ç–æ—Ä–æ–∂–Ω—ã–µ —à–∞–∂–∫–∏ (1-3)",
            'chaotic': "–•–∞–æ—Ç–∏—á–Ω–æ–µ –±–µ—à–µ–Ω—Å—Ç–≤–æ (7-12)",
            'zigzag': "–ó–∏–≥–∑–∞–≥–æ–æ–±—Ä–∞–∑–Ω–∞—è –ø–æ—Ö–æ–¥–∫–∞ (¬±3)",
            'gallop': "–†–∏—Ç–º–∏—á–Ω—ã–π –≥–∞–ª–æ–ø (2,4,2,4...)",
            'spiral': "–°–ø–∏—Ä–∞–ª—å–Ω–æ–µ –¥–≤–∏–∂–µ–Ω–∏–µ (1‚Üí5‚Üí1)",
            'sleepy': "–°–æ–Ω–Ω–æ–µ –≤—è–ª–æ–µ —Ç–æ–ø—Ç–∞–Ω–∏–µ (—á–∞—Å—Ç–æ 0)"
        }
        
        # –ò–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä –∫–æ—Ç–∞ (—É–Ω–∏–∫–∞–ª—å–Ω—ã–π —Ö—ç—à)
        cat_id_str = f"{name}_{style}_{seed}_{datetime.now().timestamp()}"
        self.cat_id = hashlib.md5(cat_id_str.encode()).hexdigest()[:8]
        
        print(f"   ID –∫–æ—Ç–∞: {self.cat_id}")
        print(f"   –°–ª–µ–¥—ã –±—É–¥—É—Ç —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã –≤: {os.path.abspath(save_dir)}/")
    
    def _generate_shifts(self, length):
        """–ì–µ–Ω–µ—Ä–∏—Ä—É–µ—Ç —Å–¥–≤–∏–≥–∏ —Å–æ–≥–ª–∞—Å–Ω–æ –≤—ã–±—Ä–∞–Ω–Ω–æ–º—É —Å—Ç–∏–ª—é."""
        random.seed(self.seed)
        
        if self.style == 'random':
            return [random.randint(1, 9) for _ in range(length)]
        
        elif self.style == 'careful':
            return [random.randint(1, 3) for _ in range(length)]
        
        elif self.style == 'chaotic':
            return [random.randint(7, 12) for _ in range(length)]
        
        elif self.style == 'zigzag':
            shifts = []
            for i in range(length):
                if i % 2 == 0:
                    shifts.append(3)
                else:
                    shifts.append(-3)
            return shifts
        
        elif self.style == 'gallop':
            pattern = [2, 4, 2, 4, 6]
            return [pattern[i % len(pattern)] for i in range(length)]
        
        elif self.style == 'spiral':
            shifts = []
            direction = 1
            current = 1
            for _ in range(length):
                shifts.append(current)
                current += direction
                if current >= 5:
                    direction = -1
                elif current <= 1:
                    direction = 1
            return shifts
        
        elif self.style == 'sleepy':
            shifts = []
            for _ in range(length):
                if random.random() < 0.4:
                    shifts.append(0)
                else:
                    shifts.append(random.randint(1, 4))
            return shifts
        
        else:
            return [random.randint(1, 9) for _ in range(length)]
    
    def _create_paw_print(self, operation_type, text, shifts, result):
        """–°–æ–∑–¥–∞—ë—Ç –æ—Ç–ø–µ—á–∞—Ç–æ–∫ –ª–∞–ø—ã (—Å–ª–æ–≤–∞—Ä—å —Å –¥–∞–Ω–Ω—ã–º–∏)."""
        return {
            'cat_id': self.cat_id,
            'cat_name': self.name,
            'style': self.style,
            'style_description': self.style_descriptions.get(self.style, ""),
            'seed': self.seed,
            'operation': operation_type,  # 'encrypt' –∏–ª–∏ 'decrypt'
            'timestamp': datetime.now().isoformat(),
            'original_length': len(text),
            'original_text': text if len(text) < 50 else text[:47] + "...",
            'result_text': result if len(result) < 50 else result[:47] + "...",
            'shifts_sample': shifts[:20],  # –ü–µ—Ä–≤—ã–µ 20 —Å–¥–≤–∏–≥–æ–≤ –¥–ª—è –ø—Ä–∏–º–µ—Ä–∞
            'avg_shift': sum(abs(s) for s in shifts) / len(shifts) if shifts else 0,
            'shift_stats': {
                'min': min(shifts) if shifts else 0,
                'max': max(shifts) if shifts else 0,
                'positive': sum(1 for s in shifts if s > 0),
                'negative': sum(1 for s in shifts if s < 0),
                'zero': sum(1 for s in shifts if s == 0)
            },
            'paw_signature': self.paw_signature
        }
    
    def _save_paw_print(self, paw_print):
        """–°–æ—Ö—Ä–∞–Ω—è–µ—Ç –æ—Ç–ø–µ—á–∞—Ç–æ–∫ –ª–∞–ø—ã –≤ JSON —Ñ–∞–π–ª."""
        # –§–æ—Ä–º–∏—Ä—É–µ–º –∏–º—è —Ñ–∞–π–ª–∞
        timestamp = datetime.now().strftime("%Y%m%d_%H%M%S_%f")
        filename = f"{self.cat_id}_{self.style}_{timestamp}.json"
        filepath = os.path.join(self.save_dir, filename)
        
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º –≤ JSON
        with open(filepath, 'w', encoding='utf-8') as f:
            json.dump(paw_print, f, ensure_ascii=False, indent=2)
        
        return filepath
    
    def encrypt(self, text, style=None, save=True):
        """–®–∏—Ñ—Ä–æ–≤–∞–Ω–∏–µ —Å —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ–º –æ—Ç–ø–µ—á–∞—Ç–∫–∞."""
        if style:
            self.style = style
        
        if not text:
            return ""
        
        shifts = self._generate_shifts(len(text))
        encrypted_chars = []
        
        for i, char in enumerate(text):
            shift = shifts[i]
            new_char = chr(ord(char) + shift)
            encrypted_chars.append(new_char)
        
        encrypted_text = ''.join(encrypted_chars)
        
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º –≤ –∏—Å—Ç–æ—Ä–∏—é
        self.history.append({
            'type': 'encrypt',
            'style': self.style,
            'original': text,
            'encrypted': encrypted_text,
            'shifts': shifts.copy()
        })
        
        # –°–æ–∑–¥–∞—ë–º –∏ —Å–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ç–ø–µ—á–∞—Ç–æ–∫ –ª–∞–ø—ã
        if save:
            paw_print = self._create_paw_print('encrypt', text, shifts, encrypted_text)
            saved_path = self._save_paw_print(paw_print)
            print(f"üíæ –û—Ç–ø–µ—á–∞—Ç–æ–∫ –ª–∞–ø—ã —Å–æ—Ö—Ä–∞–Ω—ë–Ω: {os.path.basename(saved_path)}")
        
        return encrypted_text
    
    def decrypt(self, encrypted_text, save=True):
        """–†–∞—Å—à–∏—Ñ—Ä–æ–≤–∫–∞ —Å —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ–º –æ—Ç–ø–µ—á–∞—Ç–∫–∞."""
        if not encrypted_text:
            return ""
        
        shifts = self._generate_shifts(len(encrypted_text))
        decrypted_chars = []
        
        for i, char in enumerate(encrypted_text):
            decrypted_chars.append(chr(ord(char) - shifts[i]))
        
        decrypted_text_result = ''.join(decrypted_chars)
        
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º –≤ –∏—Å—Ç–æ—Ä–∏—é
        self.history.append({
            'type': 'decrypt',
            'style': self.style,
            'encrypted': encrypted_text,
            'decrypted': decrypted_text_result,
            'shifts': shifts.copy()
        })
        
        # –°–æ–∑–¥–∞—ë–º –∏ —Å–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ç–ø–µ—á–∞—Ç–æ–∫ –ª–∞–ø—ã
        if save:
            paw_print = self._create_paw_print('decrypt', encrypted_text, shifts, decrypted_text_result)
            saved_path = self._save_paw_print(paw_print)
            print(f"üíæ –û—Ç–ø–µ—á–∞—Ç–æ–∫ –ª–∞–ø—ã —Å–æ—Ö—Ä–∞–Ω—ë–Ω: {os.path.basename(saved_path)}")
        
        return decrypted_text_result
    
    def load_paw_prints(self, cat_id=None, style=None, limit=5):
        """–ó–∞–≥—Ä—É–∂–∞–µ—Ç –∏ –ø–æ–∫–∞–∑—ã–≤–∞–µ—Ç —Å–æ—Ö—Ä–∞–Ω—ë–Ω–Ω—ã–µ –æ—Ç–ø–µ—á–∞—Ç–∫–∏ –ª–∞–ø."""
        print(f"\nüìÇ –ó–∞–≥—Ä—É–∑–∫–∞ –æ—Ç–ø–µ—á–∞—Ç–∫–æ–≤ –ª–∞–ø –∏–∑: {self.save_dir}/")
        
        paw_files = []
        for filename in os.listdir(self.save_dir):
            if filename.endswith('.json'):
                if cat_id and cat_id not in filename:
                    continue
                if style and style not in filename:
                    continue
                paw_files.append(os.path.join(self.save_dir, filename))
        
        paw_files.sort(key=os.path.getmtime, reverse=True)  # –°–Ω–∞—á–∞–ª–∞ –Ω–æ–≤—ã–µ
        
        if not paw_files:
            print("   (–Ω–µ—Ç —Å–æ—Ö—Ä–∞–Ω—ë–Ω–Ω—ã—Ö –æ—Ç–ø–µ—á–∞—Ç–∫–æ–≤)")
            return []
        
        prints_loaded = []
        for i, filepath in enumerate(paw_files[:limit]):
            try:
                with open(filepath, 'r', encoding='utf-8') as f:
                    paw_print = json.load(f)
                
                prints_loaded.append(paw_print)
                
                print(f"\n[{i+1}] üìÑ {os.path.basename(filepath)}")
                print(f"   –ö–æ—Ç: {paw_print['cat_name']} ({paw_print['cat_id']})")
                print(f"   –°—Ç–∏–ª—å: {paw_print['style']}")
                print(f"   –û–ø–µ—Ä–∞—Ü–∏—è: {paw_print['operation']}")
                print(f"   –í—Ä–µ–º—è: {paw_print['timestamp']}")
                print(f"   –î–ª–∏–Ω–∞ —Ç–µ–∫—Å—Ç–∞: {paw_print['original_length']} —Å–∏–º–≤.")
                print(f"   –°—Ä–µ–¥–Ω–∏–π —Å–¥–≤–∏–≥: {paw_print['avg_shift']:.2f}")
                
            except Exception as e:
                print(f"   –û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ {filename}: {e}")
        
        return prints_loaded
    
    def print_statistics(self):
        """–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –ø–æ –≤—Å–µ–º —Å–æ—Ö—Ä–∞–Ω—ë–Ω–Ω—ã–º —Å–ª–µ–¥–∞–º —ç—Ç–æ–≥–æ –∫–æ—Ç–∞."""
        print(f"\nüìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê –ö–û–¢–ê {self.name} ({self.cat_id})")
        print("-" * 60)
        
        all_prints = self.load_paw_prints(cat_id=self.cat_id, limit=1000)
        
        if not all_prints:
            print("–ù–µ—Ç –¥–∞–Ω–Ω—ã—Ö –¥–ª—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏")
            return
        
        # –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ –æ–ø–µ—Ä–∞—Ü–∏—è–º
        encrypts = [p for p in all_prints if p['operation'] == 'encrypt']
        decrypts = [p for p in all_prints if p['operation'] == 'decrypt']
        
        print(f"–í—Å–µ–≥–æ –æ—Ç–ø–µ—á–∞—Ç–∫–æ–≤: {len(all_prints)}")
        print(f"  üîí –®–∏—Ñ—Ä–æ–≤–∞–Ω–∏–π: {len(encrypts)}")
        print(f"  üîì –†–∞—Å—à–∏—Ñ—Ä–æ–≤–∞–Ω–∏–π: {len(decrypts)}")
        
        # –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ —Å—Ç–∏–ª—è–º (–µ—Å–ª–∏ –∫–æ—Ç –º–µ–Ω—è–ª —Å—Ç–∏–ª–∏)
        styles_count = {}
        for p in all_prints:
            style = p['style']
            styles_count[style] = styles_count.get(style, 0) + 1
        
        if len(styles_count) > 1:
            print("\nüé≠ –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–Ω—ã–µ —Å—Ç–∏–ª–∏:")
            for style, count in styles_count.items():
                desc = self.style_descriptions.get(style, "?")
                print(f"  {style:10}: {count:3} —Ä–∞–∑ - {desc}")
        
        # –°–∞–º—ã–π –∞–∫—Ç–∏–≤–Ω—ã–π –¥–µ–Ω—å
        dates = []
        for p in all_prints:
            date = p['timestamp'].split('T')[0]
            dates.append(date)
        
        if dates:
            from collections import Counter
            date_counts = Counter(dates)
            most_active = date_counts.most_common(1)[0]
            print(f"\nüóìÔ∏è  –°–∞–º—ã–π –∞–∫—Ç–∏–≤–Ω—ã–π –¥–µ–Ω—å: {most_active[0]} ({most_active[1]} –æ–ø–µ—Ä–∞—Ü–∏–π)")
    
    def create_pawprint_report(self, output_file="pawprint_report.txt"):
        """–°–æ–∑–¥–∞—ë—Ç —Ç–µ–∫—Å—Ç–æ–≤—ã–π –æ—Ç—á—ë—Ç —Å–æ –≤—Å–µ–º–∏ –æ—Ç–ø–µ—á–∞—Ç–∫–∞–º–∏ –ª–∞–ø."""
        all_prints = self.load_paw_prints(limit=1000)
        
        with open(output_file, 'w', encoding='utf-8') as f:
            f.write("=" * 70 + "\n")
            f.write(f"–û–¢–ß–Å–¢ –û–ë –û–¢–ü–ï–ß–ê–¢–ö–ê–• –õ–ê–ü –ö–û–¢–ê-–®–ò–§–†–û–í–ê–õ–¨–©–ò–ö–ê\n")
            f.write(f"–ö–æ—Ç: {self.name} (ID: {self.cat_id})\n")
            f.write(f"–°–≥–µ–Ω–µ—Ä–∏—Ä–æ–≤–∞–Ω–æ: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}\n")
            f.write("=" * 70 + "\n\n")
            
            for i, p in enumerate(all_prints, 1):
                f.write(f"–û–¢–ü–ï–ß–ê–¢–û–ö #{i}\n")
                f.write(f"–§–∞–π–ª: {p.get('filename', 'N/A')}\n")
                f.write(f"–ö–æ—Ç: {p['cat_name']} | –°—Ç–∏–ª—å: {p['style']}\n")
                f.write(f"–û–ø–µ—Ä–∞—Ü–∏—è: {p['operation']} | –í—Ä–µ–º—è: {p['timestamp']}\n")
                f.write(f"–¢–µ–∫—Å—Ç: '{p['original_text']}'\n")
                f.write(f"–†–µ–∑—É–ª—å—Ç–∞—Ç: '{p['result_text']}'\n")
                f.write(f"–°–¥–≤–∏–≥–∏ (–ø—Ä–∏–º–µ—Ä): {p['shifts_sample']}\n")
                f.write(f"–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞: —Å—Ä.—Å–¥–≤–∏–≥={p['avg_shift']:.2f}, "
                       f"min={p['shift_stats']['min']}, max={p['shift_stats']['max']}\n")
                f.write("-" * 50 + "\n")
            
            f.write(f"\n–í—Å–µ–≥–æ –æ—Ç–ø–µ—á–∞—Ç–∫–æ–≤: {len(all_prints)}\n")
            f.write("=" * 70 + "\n")
        
        print(f"üìÑ –ü–æ–ª–Ω—ã–π –æ—Ç—á—ë—Ç —Å–æ—Ö—Ä–∞–Ω—ë–Ω –≤: {output_file}")
        return output_file

